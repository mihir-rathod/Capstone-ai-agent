# Required environment variables for the Capstone project

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=your_password
DB_NAME=mccs_analytics

# API Keys
GOOGLE_API_KEY=your_gemini_api_key
OLLAMA_API_KEY=your_ollama_api_key

# API Endpoints
OLLAMA_API_URL=https://api.ollama.ai/v1

# Model Settings
MAX_RETRIES=3
PARALLEL_GENERATION=true
GENERATION_TIMEOUT=30

# Validation Settings
MIN_CONSISTENCY_SCORE=0.8
REQUIRE_DUAL_VALIDATION=true